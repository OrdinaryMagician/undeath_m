CUT_INTRO8TXT = "¿Que?";
CUT_INTRO9TXT = "Nada.";
CUT_INTRO10TXT = "*suspiro* Es que te comía a lametazos... Eres monísime.";
CUT_INTRO11TXT = "Lo mismo digo.";
CUT_INTRO12TXT = "Bueno, menudo par. No tenemos tiempo para tontear, que hay mucho que hacer.";
