CUT_INTRO30TXT = "Que mono...";
CUT_INTRO31TXT = "Nunca entenderé por qué hueles tan... Apetecible.";
CUT_INTRO32TXT = "Secreto de hechicere, jijijiji...";
CUT_INTRO33TXT = "¿Que tal si restriegas esa cara bonita por mis sobacos para apreciarlo mejor? ¿Eh?";
CUT_INTRO34TXT = "¡Jajajaja! Y luego soy yo la que se lleva una bronca por querer tontear...";
CUT_INTRO35TXT = "Mmf...";
CUT_INTRO36TXT = "Busquemos entonces esas aguas termales.";
CUT_INTRO37TXT = "Eso, eso.";
CUT_INTRO38TXT = "Ya estoy en ello, gente.";
